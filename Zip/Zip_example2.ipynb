        "# Example 2\n",
        "# Create a dictionary using zip"
      ],
      "metadata": {
        "id": "Nsa9PxXzK-2H"
      },
      "execution_count": 1,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "names =[\"alpha\", \"beta\", \"gama\"]\n",
        "values = [10,20,30]"
      ],
      "metadata": {
        "id": "w1xsOmuhOfZS"
      },
      "execution_count": 11,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "for n,v in zip(names,values):\n",
        "  mydict = dict()\n",
        "  mydict[n] = v\n",
        "\n",
        "  print(mydict)"
      ],
