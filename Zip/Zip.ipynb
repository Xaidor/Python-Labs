    },
    {
      "cell_type": "code",
      "source": [
        "listA = [3,4,5,6,3,-17]\n",
        "listB = [\"q\",\"w\",\"e\",\"r\",\"t\",\"y\"]"
      ],
      "metadata": {
        "id": "w1xsOmuhOfZS"
      },
      "execution_count": 9,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "for a,b in zip(listA,listB):\n",
        "  mydict = dict()\n",
        "  mydict[a] = b\n",
        "\n",
        "  print(mydict)"
      ],
